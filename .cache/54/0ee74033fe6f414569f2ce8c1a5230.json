:19},"source":"src/Views/ContactContainerView.js","original":{"line":53,"column":19},"name":"ContactContainerView"},{"generated":{"line":71,"column":39},"source":"src/Views/ContactContainerView.js","original":{"line":53,"column":39}},{"generated":{"line":71,"column":41},"source":"src/Views/ContactContainerView.js","original":{"line":53,"column":41}},{"generated":{"line":72,"column":0},"source":"src/Views/ContactContainerView.js","original":{"line":53,"column":41}}],"sources":{"src/Views/ContactContainerView.js":"class ContactContainerView {\n  container = document.querySelector(\".list-container\");\n\n  render(data) {\n    this.data = data;\n    const html = this.generateHTMLString();\n    this.container.innerHTML = html;\n    this.applyDeleteEvent();\n  }\n\n  applyDeleteEvent() {\n    const deleteBtns = document.querySelectorAll(\".delete\");\n    deleteBtns.forEach((btn) => {\n      btn.addEventListener(\"click\", () => {\n        this.onDelete(btn.id);\n      });\n    });\n  }\n\n  addDeleteListner(handler) {\n    this.onDelete = handler;\n  }\n\n  pushContactIntoContainer(contact) {\n    const html = this.generatCardContactHTML(contact);\n    this.container.insertAdjacentHTML(\"afterbegin\", html);\n    this.applyDeleteEvent();\n  }\n\n  generateHTMLString() {\n    const data = this.data;\n    let html = \"\";\n    if (Array.isArray(data)) {\n      data.forEach((contact) => {\n        html += this.generatCardContactHTML(contact);\n      });\n      return html;\n    }\n  }\n\n  generatCardContactHTML(contact) {\n    return ` <div class=\"card flex between\">\n            <div class=\"info flex column start\">\n              <span>${contact.name}</span>\n              <span>${contact.phone}</span>\n              <span>${contact.email}</span>\n            </div>\n            <button class=\"delete\" id=\"${contact.id}\"><i class=\"fa-solid fa-user-minus\"></i></button>\n        </div>`;\n  }\n}\n\nexport default new ContactContainerView();"},"lineCount":null}},"e